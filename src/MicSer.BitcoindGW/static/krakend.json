{
    "version": 2,
    "extra_config": {},
    "timeout": "500ms",
    "cache_ttl": "300s",
    "output_encoding": "json",
    "name": "Bitcoind GW",
    "port": 18443,
    "endpoints": [
      {
        "endpoint": "/",
        "output_encoding": "no-op",
        "extra_config": {},
        "headers_to_pass": [
          "Authorization"
        ],
        "backend": [
          {
            "url_pattern": "/",
            "encoding": "no-op",
            "host": [
              "http://host.docker.internal:18443",
              "http://host.docker.internal:18444"
            ],
            "disable_host_sanitize": false
          }
        ],
        "method": "POST"
      }
    ]
  }